<div class="">
	{% for date, trips in calendar %}
		<div class="border-bottom d-flex mt-3">
			<div class="rounded-top bg-primary bg-opacity-10 px-2">
				{{ date|format_datetime(pattern='dd MMM', locale='fr') }}
			</div>
		</div>
		<div class="row align-items-center">
			{% for trip in trips %}

				<div class="col">
					{% include 'table/_plan_btn.html.twig' %}
				</div>
			{% else %}
				<div class="p-3"></div>
			{% endfor %}
		</div>
	{% endfor %}
</div>
