<section class="card">
    <div class="card-header">
        <h2>Participer</h2>
    </div>
    <div class="card-body">
        {{ form_start(form, { 'data-turbo-frame'="_top" }) }}
            {{ form_row(form.message) }}
            <div class="text-end">
                <button class="btn btn-success">{{ button_label|default('Envoyer') }}</button>
            </div>
        {{ form_end(form) }}
    </div>
</section>
